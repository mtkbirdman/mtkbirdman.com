FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeatureExtractDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

airplane.obj
{
    extractionMethod    extractFromSurface;
    includedAngle       180;

    subsetFeatures
    {    
        nonManifoldEdges       no;
        openEdges       yes;
    }

    writeObj            yes;
}


